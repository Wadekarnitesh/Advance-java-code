package com.jsp.unimapcascade2.driver;

import javax.persistence.EntityManager;
import javax.persistence.EntityManagerFactory;
import javax.persistence.EntityTransaction;
import javax.persistence.Persistence;

import com.jsp.unimapcascade2.entity.Bus;
import com.jsp.unimapcascade2.entity.Driver;

public class UniversityCollegeDriver{
	
	public static void main(String[] args) {
		
		EntityManagerFactory emf = Persistence.createEntityManagerFactory(null);
		
		EntityManager em = emf.createEntityManager();
		
		EntityTransaction et = em.getTransaction();
	
		Driver driver1 = new Driver(1, "Rakesh ", "LJK3231", 3);
		Driver driver2 = new Driver(2, "Manoj ", "TUO7979", 1);
		Driver driver3 = new Driver(3, "Viraj ", "RDY23232", 0);
		Driver driver4 = new Driver(4, "Shivam ", "NHWE3422", 2);
		Driver driver5 = new Driver(5, "Tapu ", "F3332J3", 9);
	
		Bus bus1 = new Bus(02, "MH11OP9849", "Hybrid", driver4);
		Bus bus2 = new Bus(03, "MH11KL4559", "Petrol", driver5);
		Bus bus3 = new Bus(05, "MH11NM4999", "CNG", driver3);
		Bus bus4 = new Bus(05, "MH11UY7649", "LPG", driver1);
		Bus bus5 = new Bus(06, "MH11CH1234", "Diesel", driver2);
		
		
		
		
	}
	

}
